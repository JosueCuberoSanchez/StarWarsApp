target 'StarWarsApp' do

  pod 'RxSwift', '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  pod 'R.swift'
  pod 'SwiftLint'
  pod 'RxOptional'

  target 'StarWarsAppTests' do

     pod 'RxBlocking', '~> 4.0'
     pod 'RxTest',     '~> 4.0'

  end

end
